<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.toughguy.dataDisplay.model.content.DictXZQHB">

	<!-- 查询某个信息 -->
	<select id="findAll" parameterType="Map" resultType="DictXZQHB">
		select * from DICT_XZQHB
	</select>
	<update id="update" parameterType="DictXZQHB">
		update DICT_XZQHB
		<set>
			<if test="xzqhdm != null and sampleNo != ''">sampleNo = #{sampleNo},</if>
			<if test="xzqhmc != null and position != ''">position = #{position},</if>
			<if test="jjslsx != null and otherState != ''">otherState = #{otherState},</if>
		</set>
		where xzqhdm = #{xzqhdm}
	</update>
	

	
	
	
	
</mapper>